{
  "name": "sv-restaurant-zug-bot",
  "version": "0.1.0",
  "description": "A Telegram bot to retrieve the menu of the SV restaurant in Zug (called Five Moods)",
  "engines": {
    "node": ">=6.11.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node app/app.js",
    "start-debug": "node --inspect app/app.js",
    "lint": "jshint --reporter=node_modules/jshint-stylish app",
    "test": "mocha test/",
    "test-debug": "mocha test/ --inspect",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirius-A/sv-restaurant-zug-bot.git"
  },
  "author": "Fabio Zuber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sirius-A/sv-restaurant-zug-bot/issues"
  },
  "homepage": "https://github.com/Sirius-A/sv-restaurant-zug-bot/readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "co": "^4.6.0",
    "cron": "^1.2.1",
    "mongodb": "^v2.2.30",
    "request": "^2.80.0",
    "tgfancy": "^0.11.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "glob": "^7.1.1",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "mocha": "^3.4.2"
  },
  "jshintConfig": {
    "esversion": 6,
    "node": true,
    "mocha": true
  }
